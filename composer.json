{
    "name": "klepak/enterprise-driver-management",
    "type": "library",
    "authors": [
        {
            "name": "Knut Leborg",
            "email": "knut@lepa.no"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "5.8.*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Klepak\\DriverManagement\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Klepak\\DriverManagement\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klepak\\DriverManagement\\DriverManagementServiceProvider"
            ]
        }
    }
}
